{
    "name": "fas/configuration",
    "type": "library",
    "description": "Fast and simple configuration management",
    "keywords": [
        "configuration",
        "yaml",
        "json"
    ],
    "homepage": "https://github.com/gielfeldt/fas-routing",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gielfeldt",
            "email": "thomas@gielfeldt.dk"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "symfony/yaml": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Fas\\Configuration\\": "src/Configuration/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Fas\\Configuration\\Tests\\": "tests/Configuration/"
        }
    }
}
